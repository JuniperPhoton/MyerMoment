        <Grid x:Name="StyleGrid" Visibility="Collapsed" Grid.Row="2" Grid.RowSpan="2" RenderTransformOrigin="0.5,0.5" VerticalAlignment="Bottom" Background="#FFE0E0E0" >
            <Grid.RenderTransform>
                <CompositeTransform TranslateY="400"/>
            </Grid.RenderTransform>
            <Border BorderBrush="{StaticResource MomentThemeColor}" BorderThickness="1" Margin="0" Background="{StaticResource MomentThemeColor}" Height="5" VerticalAlignment="Top"/>
            <StackPanel  Orientation="Vertical">
                <TextBlock  x:Uid="styleHeader" Text="STYLE" Foreground="{StaticResource MomentThemeBlack}" FontWeight="ExtraBold" FontSize="20" Margin="10" FontFamily="Segoe WP"/>
                <Grid VerticalAlignment="Top" >
                    <StackPanel Orientation="Vertical">
                        <ScrollViewer x:Name="styleSV" HorizontalScrollMode="Auto" HorizontalScrollBarVisibility="Visible">
                            <StackPanel x:Name="styleSP" Orientation="Horizontal" Margin="0,0,10,0">
                                <Button Click="StyleClick" Width="70" Height="70" Margin="10,10,0,0" BorderThickness="0" Style="{StaticResource ButtonStyle2}" MinHeight="10" MinWidth="10" VerticalAlignment="Top">
                                    <Border Background="White">
                                        <TextBlock x:Uid="customTB"  Text="Custom" Foreground="{StaticResource MomentThemeBlack}" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                    </Border>
                                </Button>
                            </StackPanel>
                        </ScrollViewer>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>

                        </Grid>
                    </StackPanel>
                </Grid>
            </StackPanel>

        </Grid>
        <Grid x:Name="FilterGrid" d:IsHidden="True"  Visibility="Visible" Grid.Row="2" Grid.RowSpan="2" RenderTransformOrigin="0.5,0.5" VerticalAlignment="Bottom" Background="#FFE0E0E0" >
            <Grid.RenderTransform>
                <CompositeTransform TranslateY="400"/>
            </Grid.RenderTransform>

            <Border BorderBrush="{StaticResource MomentThemeColor}" BorderThickness="1" Margin="0" Background="{StaticResource MomentThemeColor}" Height="5" VerticalAlignment="Top"/>

            <StackPanel  Orientation="Vertical">
                <TextBlock  x:Uid="filterHeader" Text="FILTER" Foreground="{StaticResource MomentThemeBlack}" FontWeight="ExtraBold" FontSize="20" Margin="10" FontFamily="Segoe WP"/>
                <Grid VerticalAlignment="Top" >
                    <StackPanel Orientation="Vertical">
                        <ScrollViewer x:Name="filterSV"  HorizontalScrollMode="Auto" HorizontalScrollBarVisibility="Visible">
                            <StackPanel x:Name="filterSP" Orientation="Horizontal" Margin="0,0,10,0">
                                
                               
                            </StackPanel>
                        </ScrollViewer>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>

                        </Grid>
                    </StackPanel>
                </Grid>
            </StackPanel>

        </Grid>

        <Grid x:Name="MaskGrid" Grid.Row="0"  Grid.RowSpan="4" Margin="0,-52,0,0" Visibility="{Binding ShowLoadingVisibility}" d:IsHidden="True">
            <Border Background="Black" Opacity="0.8"/>
            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Orientation="Vertical">
                <ProgressRing IsActive="True" HorizontalAlignment="Center" Background="Transparent" Foreground="{StaticResource MomentThemeColor}"/>
                <TextBlock x:Name="savingTB" Text="Working on saving..." FontSize="30" FontWeight="SemiLight" />
            </StackPanel>
        </Grid>
        <Grid x:Name="ErrorGrid" Grid.Row="0"  Grid.RowSpan="4" Margin="0,-52,0,0" Visibility="{Binding ShowErrorVisibility}" d:IsHidden="True">
            <Border Background="Black" Opacity="0.8"/>
            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Stretch" Orientation="Vertical">
                <TextBlock VerticalAlignment="Center" x:Name="errorTB" Text="ERROR :-(" FontSize="40" HorizontalAlignment="Center" FontWeight="ExtraBlack" />
                <TextBlock VerticalAlignment="Center" x:Name="errorHintTB" Text="For some reason, this picture should be compressed." FontSize="20" TextWrapping="Wrap" HorizontalAlignment="Center" TextAlignment="Center" FontWeight="ExtraLight" Margin="0,10" />
                <Button x:Name="retryBtn" Click="retryClick" Height="50" MinHeight="10"  Style="{StaticResource ButtonStyle2}" BorderThickness="0" Margin="10" HorizontalAlignment="Stretch">
                    <Border Background="{StaticResource MomentThemeColor}">
                        <TextBlock x:Name="retryTB" Foreground="{StaticResource MomentThemeBlack}" Text="Compress and retry" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Border>
                </Button>
                <Button Click="backErrorClick" Height="50" MinHeight="10"  Style="{StaticResource ButtonStyle2}" BorderThickness="0" Margin="10" HorizontalAlignment="Stretch">
                    <Border Background="White">
                        <TextBlock x:Name="backTB" Foreground="{StaticResource MomentThemeBlack}" Text="Return" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Border>
                </Button>
            </StackPanel>
        </Grid>
        <Grid x:Name="ShareGrid"  Margin="0,-52,0,0" Visibility="{Binding ShowShareVisibility}" Grid.Row="0"  Grid.RowSpan="6" d:IsHidden="True">
            <Border Background="Black" Opacity="0.8"/>
            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Stretch" Orientation="Vertical">
                <TextBlock x:Name="savedTB" HorizontalAlignment="Center" Text="Photo saved :D" FontSize="30" />
                <Button  x:Name="shareToWeiboBtn"  Click="ShareToWeiboClick" Height="50" MinHeight="10"  Style="{StaticResource ButtonStyle2}" BorderThickness="0" Margin="10" HorizontalAlignment="Stretch">
                    <Border Background="{StaticResource MomentThemeColor}">
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                            <!--<Image Source="Asset/Icon/weibo.png" Width="30" Height="30"/>-->
                            <TextBlock x:Uid="shareToWeiboTB" Foreground="{StaticResource MomentThemeBlack}" Text="SHARE TO WEIBO" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0"/>
                        </StackPanel>
                    </Border>
                </Button>
                <Button x:Name="shareToWechatBtn" Click="ShareToWechatClick"  Height="50" MinHeight="10"  Style="{StaticResource ButtonStyle2}" BorderThickness="0" Margin="10" HorizontalAlignment="Stretch">
                    <Border Background="#FFFFDC7F">
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                            <!--<Image Source="Asset/Icon/wechat.png" Width="30" Height="30"/>-->
                            <TextBlock x:Uid="shareToWechatTB" Foreground="{StaticResource MomentThemeBlack}" Text="SHARE TO WECHAT" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0"/>
                        </StackPanel>
                    </Border>
                </Button>
                <!--<Button Click="ShareToSystemClick" x:Name="systemBtn" Height="50" MinHeight="10"  Style="{StaticResource ButtonStyle2}" BorderThickness="0" Margin="10" HorizontalAlignment="Stretch">
                    <Border Background="#FFFFD360">
                        <TextBlock x:Uid="systemShareTB" Foreground="{StaticResource MomentThemeBlack}" Text="SHARE" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Border>
                </Button>-->
                <Button x:Name="backHomeBtn" Click="BackHomeClick" Height="50" MinHeight="10"  Background="White" Style="{StaticResource ButtonStyle2}" BorderThickness="0" Margin="10" HorizontalAlignment="Stretch">
                    <Border Background="White">
                        <TextBlock x:Name="backhomeTB" Foreground="{StaticResource MomentThemeBlack}" Text="BACK TO HOME" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Border>
                </Button>
            </StackPanel>
        </Grid>
    </Grid>
</Page>
